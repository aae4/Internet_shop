class UserSessionController < ApplicationController
	def index
	end
	
	def create
		user=User.auth(params[:login],params[:password]
		if user
			self.current_user=user
			flash[:notice]="Succes login"
		else
			@login=params[:login]
			render :action => 'new'
		end
	end

	def destroy
		

				
		
end
